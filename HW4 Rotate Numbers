#include<stdio.h>
#include<string.h>

int main(){
char a[101];
for(int i=0;i<101;i++)
    a[i]='0';

int flag=0;
int flag1=0;
scanf("%s",&a);
/*for(int i=0;i<101;i++)
    printf("%c",a[i]);
printf("\n");*/

for(int i=0;i<101;i++)
    if(a[i]=='0')
        flag1++;
if(flag1==100)
{
    printf("No\n");
    exit(1);
}
for(int i=0;i<100;i++)
    if(a[i]=='2'||a[i]=='3'||a[i]=='4'||a[i]=='5'||a[i]=='7')
    {
     printf("No\n");
     exit(1);
    }

for(int i=99;i>=0;i--)
{
     if(a[i]!='0'&&a[i]!='\0')
    {

        flag=i;
        break;
    }
}

for(int i=flag;i>=0;i--)
{
    if(a[i]=='1')
        printf("1");
    if(a[i]=='6')
        printf("9");
    if(a[i]=='8')
        printf("8");
    if(a[i]=='0')
        printf("0");
    if(a[i]=='9')
        printf("6");

}
    printf("\n");

return 0;
}
