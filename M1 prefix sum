#include<stdio.h>
int main(){
int x;
scanf("%d",&x);
int y[x];
for(int i=0;i<x;i++)
{
    scanf("%d",&y[i]);
    //printf("%d",y[i]);
}

//printf("\n");
int time;
scanf("%d",&time);
//printf("%d",time);
int ta[time][2];
for(int i=0;i<time;i++)
    for(int j=0;j<2;j++)
        scanf("%d",&ta[i][j]);

       // for(int i=0;i<time;i++)
           // for(int j=0;j<2;j++)
               // printf("%d",ta[i][j]);


int an=0;
for(int i=0;i<time;i++){
        an=0;
      for(int j=ta[i][0]-1;j<=ta[i][1]-1;j++)
        an = an+y[j];
printf("%d\n",an);
}






return 0;
}
