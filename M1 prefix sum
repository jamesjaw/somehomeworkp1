#include<stdio.h>
int main(){
   int n;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   int t;
   int anw;
   scanf("%d",&t);
   int L,R;
   for(int i=0;i<t;i++)
   {
       anw = 0;
       scanf("%d%d",&L,&R);
      for(int j=L-1;j<=R-1;j++)
        anw = anw+a[j];
   printf("%d\n",anw);
   }
return 0;
}
